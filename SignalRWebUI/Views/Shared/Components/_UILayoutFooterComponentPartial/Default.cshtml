@model List<ResultContactDto>
@foreach (var item in Model)
{

<footer class="footer_section">
    <div class="container">
        <div class="row">
            <div class="col-md-4 footer-col">
                <div class="footer_contact">
                    <h4>
                        Bize Ulaşın
                    </h4>
                    <div class="contact_link_box">
                        <a href="@item.Location">
                            <i class="fa fa-map-marker" aria-hidden="true"></i>
                            <span>
                                Lokasyon
                            </span>
                        </a>
                        <a href="">
                            <i class="fa fa-phone" aria-hidden="true"></i>
                            <span>
                                @item.Phone
                            </span>
                        </a>
                        <a href="">
                            <i class="fa fa-envelope" aria-hidden="true"></i>
                            <span>
                                @item.Mail
                            </span>
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-md-4 footer-col">
                <div class="footer_detail">
                    <a href="/Default/Index/" class="footer-logo">
                        @item.FooterTitle
                    </a>
                    <p1>
                        @item.FooterDescription
                    </p1>
                    <div class="footer_social">
                        <a href="">
                            <i class="fa fa-facebook" aria-hidden="true"></i>
                        </a>
                        <a href="">
                            <i class="fa fa-twitter" aria-hidden="true"></i>
                        </a>
                        <a href="">
                            <i class="fa fa-linkedin" aria-hidden="true"></i>
                        </a>
                        <a href="">
                            <i class="fa fa-instagram" aria-hidden="true"></i>
                        </a>
                        <a href="">
                            <i class="fa fa-pinterest" aria-hidden="true"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-md-4 footer-col">
                <h4>
						@item.OpenDays
                </h4>
                <p1>
                    @item.OpenDaysDescription
                </p1>
                <p1>
                    @item.OpenHours
                </p1>
            </div>
        </div>
        <div class="footer-info">
            <p1>
                &copy; <span id="displayYear"></span> Tüm Hakları Saklıdır.
                <a href="https://html.design/">SignalR Restoran</a><br><br>
                &copy; <span id="displayYear"></span> Geliştiren 
                <a href="https://themewagon.com/" target="_blank">Bilal Karagün</a>
            </p1>
        </div>
    </div>
</footer>}